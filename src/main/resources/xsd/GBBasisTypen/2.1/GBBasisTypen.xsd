      <xsd:element name="Ursprung" type="RechtRefBegruendet" minOccurs="0"/>
      <xsd:element ref="extensions" minOccurs="0"/>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:ID"/>
    <xsd:anyAttribute processContents="lax"/>
