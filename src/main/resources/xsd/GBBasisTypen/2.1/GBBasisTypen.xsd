        <xsd:complexType>
          <xsd:sequence>
            <xsd:element ref="RangNachfolge" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element ref="Vorrang" minOccurs="0" maxOccurs="unbounded"/>
